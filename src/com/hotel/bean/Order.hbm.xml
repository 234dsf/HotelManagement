<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.hotel.bean.Order" table="order">
		<id name="orderid" type="java.lang.Integer" column="orderid">
			<generator class="identity"></generator>
		</id>
		
		<property name="userid" type="java.lang.Integer">
			<column name="userid" length="45" not-null="false">
				<comment>用户名</comment>
			</column>
		</property>
		
		<property name="roomid" type="java.lang.Integer">
			<column name="roomid" length="45" not-null="false">
				<comment>密码</comment>
			</column>
		</property>
		
	</class>

</hibernate-mapping>